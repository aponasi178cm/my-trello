d from './CardAdd'\nimport Card from './Card'\n// ★ここから追記\nexport default {\n  components: {\n    CardAdd,\n    Card,\n    draggable,\n  },\n  props: {\n    title: {\n      type: String,\n      required: true\n    },\n    cards:{\n      type: Array,\n      required:true\n    },\n    listIndex: {\n      type: Number,\n      required: true\n    }\n  },\n  methods: {\n    removeList: function() {\n      if(confirm('本当にこのリストを削除しますか？')){\n        this.$store.dispatch('removelist', { listIndex: this.listIndex })\n      }\n    },\n  },\n  computed:{\n    totalCardList:function(){\n        return this.cards.length;\n    }\n  }\n}\n// ★ここまで追記\n</script>"]}]}